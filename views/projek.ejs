<!DOCTYPE html>
<html lang="en">

<head>

<%- include('partials/meta') %>
<%- include('partials/linkcss') %>

</head>


<body id="page-top">
<%- include('partials/navbar') %>
<div class="container">
  <div class="row">
    <% for(var x=0; x < projek.length; x++ ){ %>
        <div class="col-sm-6 col-md-4 col-lg-3 mt-4">
          <div class="card">
              <img class="card-img-top" src="<%= projek[x].logo %> ">
              <div class="card-block padding">
                  <h4 class="card-title"><%= projek[x].company%></h4>
                  <div class="card-text">
                      <p><%= projek[x].description%></p>
                      <p style="font-size: small;"><b><%= projek[x].tech%></b></p>
                  </div>
              </div>
              <div class="card-footer">
                  <span class="text-muted"><%= projek[x].category%></span>
              </div>
          </div>
      </div>
    <% } %>
  </div>
</div>


<%- include('partials/footer') %>

</body>
</html>
